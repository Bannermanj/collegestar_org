<%= form_for @module_proposal do |f| %>
  <%= render 'module_proposals/forms/fields/name', f: f  %>
  <%= render 'module_proposals/forms/fields/email', f: f %>
  <%= render 'module_proposals/forms/fields/university', f: f %>
  <%= render 'module_proposals/forms/fields/department', f: f %>
  <hr>
  <%= render 'module_proposals/forms/fields/title', f: f %>
  <%= render 'module_proposals/forms/fields/description', f: f %>
  <%= render 'module_proposals/forms/fields/start_date', f: f %>
  <%= render 'module_proposals/forms/fields/completion_date', f: f %>
  
  <%= render 'module_proposals/forms/fields/new_submit', f: f %>
<% end %>
