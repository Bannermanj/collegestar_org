<%= form_for @redesign_summary, url: profile_redesign_summary_path(profile_id: @current_user, id: @redesign_summary)  do |f| %>
  <%= render 'shared/redesign_summaries/forms/fields/uuid', f: f %>
  <%= render 'shared/redesign_summaries/forms/fields/questions', f: f %>
  <%= render 'shared/redesign_summaries/forms/attachment_drop_zone' %>
  <%= render 'shared/redesign_summaries/forms/submit', f: f %>
<% end %>
<%= render 'shared/redesign_summaries/forms/attachment_upload' %>
